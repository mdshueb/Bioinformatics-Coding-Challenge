    {
      "cell_type": "code",
      "source": [
        "# c.Specificity calculation for top 10 PMPs\n",
        "import pandas as pd\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.metrics import confusion_matrix\n",
        "\n",
        "# Calculate coverage for each CpG site in each tissue\n",
        "for tissue in data['Tissue'].unique():\n",
        "    tissue_data = data[data['Tissue'] == tissue]\n",
        "    for cpg_index in range(3):\n",
        "        # Calculate coverage for the current CpG site\n",
        "        coverage = tissue_data.apply(\n",
        "            lambda row: calculate_cpg_coverage(row, cpg_index), axis=1\n",
        "        )\n",
        "\n",
        "        # Update the original DataFrame with the calculated coverage\n",
        "        data.loc[tissue_data.index, f'CpG_{cpg_index + 1}_Coverage'] = coverage\n",
        "\n",
        "# Assuming 'data' is your DataFrame and 'Tissue' is your tissue column\n",
        "# Extract CpG_1_Coverage as the feature and Tissue as the target\n",
        "X_cpg1 = data[['CpG_1_Coverage']]\n",
        "y = data['Tissue']\n",
        "\n",
        "# Split data into training and testing sets\n",
        "X_train_cpg1, X_test_cpg1, y_train, y_test = train_test_split(X_cpg1, y, test_size=0.2, random_state=42)\n",
        "\n",
        "# Train logistic regression model using CpG_1_Coverage\n",
        "model_cpg1 = LogisticRegression()\n",
        "model_cpg1.fit(X_train_cpg1, y_train)\n",
        "\n",
        "# Get predictions on the test set\n",
        "y_pred_cpg1 = model_cpg1.predict(X_test_cpg1)\n",
        "\n",
        "# Get confusion matrix for the CpG_1_Coverage model\n",
        "conf_matrix_cpg1 = confusion_matrix(y_test, y_pred_cpg1)\n",
        "\n",
        "# Calculate specificity for Tissue #2\n",
        "# Specificity = True Negatives / (True Negatives + False Positives)\n",
        "# In the confusion matrix:\n",
        "# - True Negatives are at index [1, 1] (assuming Tissue #2 is labeled as 1)\n",
        "# - False Positives are at index [1, 0]\n",
        "specificity_cpg1 = conf_matrix_cpg1[1, 1] / (conf_matrix_cpg1[1, 1] + conf_matrix_cpg1[1, 0])\n",
        "\n",
        "print(f\"Specificity for Tissue #2 using CpG_1_Coverage model: {specificity_cpg1}\")"
      ],
