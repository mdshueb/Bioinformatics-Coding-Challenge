        "import os\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import subprocess\n",
        "from pysam import VariantFile\n",
        "\n",
        "# Function to run shell commands\n",
        "def run_command(command):\n",
        "    print(f\"Running: {command}\")\n",
        "    process = subprocess.run(command, shell=True, check=True, stdout=subprocess.PIPE, stderr=subprocess.PIPE)\n",
        "    return process.stdout.decode().strip()\n",
        "\n",
        "# Step 1: Variant Calling with Bcftools\n",
        "def call_variants(reference, tumor_bam, normal_bam, output_vcf):\n",
        "    # Run bcftools for variant calling\n",
        "    command = (\n",
        "        f\"bcftools mpileup -f {reference} {tumor_bam} {normal_bam} | \"\n",
        "        f\"bcftools call -mv -Oz -o {output_vcf}\"\n",
        "    )\n",
        "    run_command(command)\n",
        "\n",
        "# Step 2: Parse VCF File\n",
        "def parse_vcf(vcf_file):\n",
        "    variants = []\n",
        "    with VariantFile(vcf_file) as vcf:\n",
        "        for record in vcf:\n",
        "            variants.append({\n",
        "                \"CHROM\": record.chrom,\n",
        "                \"POS\": record.pos,\n",
        "                \"REF\": record.ref,\n",
        "                \"ALT\": \",\".join(record.alts)\n",
        "            })\n",
        "    return pd.DataFrame(variants)\n",
        "\n",
        "# Step 3: Identify Somatic Mutations\n",
        "def identify_somatic_mutations(tumor_vcf, normal_vcf):\n",
        "    tumor_variants = parse_vcf(tumor_vcf)\n",
        "    normal_variants = parse_vcf(normal_vcf)\n",
        "\n",
        "    # Find somatic mutations (present in tumor, absent in normal)\n",
        "    somatic_variants = tumor_variants.merge(normal_variants, on=[\"CHROM\", \"POS\", \"REF\", \"ALT\"], how=\"outer\", indicator=True)\n",
        "    somatic_variants = somatic_variants[somatic_variants[\"_merge\"] == \"left_only\"].drop(columns=[\"_merge\"])\n",
        "    return somatic_variants\n",
        "\n",
        "# Step 4: Calculate Background Mutation Level\n",
        "def calculate_background_mutation_level(normal_vcf):\n",
        "    normal_variants = parse_vcf(normal_vcf)\n",
        "    mutation_count = normal_variants.shape[0]\n",
        "    median_background = mutation_count / 1e6  # Normalized to reads per million\n",
        "    return median_background\n",
        "\n",
        "# Step 5: Confidence Threshold for Mutation Calling\n",
        "def calculate_required_reads(confidence_threshold, median_background):\n",
        "    return confidence_threshold / median_background\n",
        "\n",
        "# Main Workflow\n",
        "if __name__ == \"__main__\":\n",
        "    # File paths\n",
        "    reference_genome = \"hg38.fa\"\n",
        "    tumor_bam = \"tumor.sorted.bam\"\n",
        "    normal_bam = \"normal.sorted.bam\"\n",
        "    tumor_vcf = \"tumor.vcf.gz\"\n",
        "    normal_vcf = \"normal.vcf.gz\"\n",
        "\n",
        "    # Call variants for tumor\n",
        "    call_variants(reference_genome, tumor_bam, normal_bam, tumor_vcf)\n",
        "    # Call variants for normal - This was missing, causing the error\n",
        "    call_variants(reference_genome, normal_bam, normal_bam, normal_vcf)\n",
        "\n",
        "    # Index the VCF\n",
        "    run_command(f\"bcftools index {tumor_vcf}\")\n",
        "    run_command(f\"bcftools index {normal_vcf}\")\n",
        "\n",
        "    # Parse and analyze VCF files\n",
        "    somatic_variants = identify_somatic_mutations(tumor_vcf, normal_vcf)\n",
        "    print(f\"Somatic Variants:\\n{somatic_variants}\")\n",
        "\n",
        "    # Calculate background mutation level\n",
        "    median_background = calculate_background_mutation_level(normal_vcf)\n",
        "    print(f\"Median Background Mutation Level: {median_background}\")\n",
        "\n",
        "    # Determine required reads for confidence\n",
        "    confidence_threshold = 10\n",
        "    required_reads = calculate_required_reads(confidence_threshold, median_background)\n",
        "    print(f\"Required Reads per Million: {required_reads}\")"
