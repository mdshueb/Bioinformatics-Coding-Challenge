    {
      "cell_type": "code",
      "source": [
        "#Loading the CSV Dataset\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Load the dataset\n",
        "file_path = '/content/PupilBioTest_PMP_revA.csv'  # Update with your actual file path\n",
        "\n",
        "# Attempt to read the CSV with error handling\n",
        "try:\n",
        "    data = pd.read_csv(file_path, sep=',', quotechar='\"', escapechar='\\\\')\n",
        "except pd.errors.ParserError:\n",
        "    # If ParserError occurs, try skipping bad lines\n",
        "    print(\"ParserError encountered. Attempting to skip bad lines...\")\n",
        "    data = pd.read_csv(file_path, sep=',', quotechar='\"', escapechar='\\\\', on_bad_lines='skip')\n",
        "\n",
        "# Print some info to verify the data has been loaded\n",
        "print(f\"Data loaded successfully. DataFrame shape: {data.shape}\")\n",
        "data.head()\n",
        "\n",
        "# Assuming the methylation status columns are named '`000'`, '`001'`, etc.\n",
        "methylation_columns = ['`000', '`001', '`010', '`011', '`100', '`101', '`110', '`111']\n",
        "\n",
        "# Function to calculate coverage for a single CpG site\n",
        "def calculate_cpg_coverage(row, cpg_index):\n",
        "    \"\"\"Calculates coverage for a single CpG site based on methylation patterns.\n",
        "\n",
        "    Args:\n",
        "        row: A row from the DataFrame representing a sample.\n",
        "        cpg_index: The index of the CpG site (0, 1, or 2).\n",
        "\n",
        "    Returns:\n",
        "        The coverage for the specified CpG site in the given sample.\n",
        "    \"\"\"\n",
        "    coverage = 0\n",
        "    for column in methylation_columns:\n",
        "        if column[cpg_index + 1] == '1':  # Check if the CpG site is methylated in this pattern\n",
        "            coverage += int(row[column])\n",
        "    return coverage"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "eVkEyyBDtWKY",
        "outputId": "c34569af-678a-4af3-f3d6-77cadc04d4e2"
      },
      "execution_count": 4,
