        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Calculate coverage for each CpG site in each tissue\n",
        "for tissue in data['Tissue'].unique():\n",
        "    # Select rows for the current tissue\n",
        "    tissue_indices = data['Tissue'] == tissue\n",
        "\n",
        "    for cpg_index in range(3):\n",
        "        coverage_column = f'CpG_{cpg_index + 1}_Coverage'\n",
        "\n",
        "        # Apply function and assign directly to the original DataFrame\n",
        "        data.loc[tissue_indices, coverage_column] = data.loc[tissue_indices].apply(\n",
        "            lambda row: calculate_cpg_coverage(row, cpg_index), axis=1\n",
        "        )\n",
        "\n",
        "    # Calculate median and coefficient of variation for each CpG site\n",
        "    for cpg_index in range(3):\n",
        "        coverage_column = f'CpG_{cpg_index + 1}_Coverage'\n",
        "\n",
        "        # Compute stats using the subset of data\n",
        "        tissue_data = data.loc[tissue_indices]\n",
        "        median_coverage = tissue_data[coverage_column].median()\n",
        "        cv_coverage = tissue_data[coverage_column].std() / tissue_data[coverage_column].mean()\n",
        "\n",
        "        print(f\"Tissue: {tissue}, CpG Site: {cpg_index + 1}\")\n",
        "        print(f\"Median Coverage: {median_coverage}\")\n",
        "        print(f\"Coefficient of Variation: {cv_coverage}\\n\")\n",
        "\n",
        "    # Generate plots for coverage statistics (example using box plots)\n",
        "    plt.figure(figsize=(8, 6))\n",
        "    plt.boxplot(\n",
        "        [\n",
        "            data.loc[tissue_indices, 'CpG_1_Coverage'],\n",
        "            data.loc[tissue_indices, 'CpG_2_Coverage'],\n",
        "            data.loc[tissue_indices, 'CpG_3_Coverage']\n",
        "        ],\n",
        "        labels=['CpG 1', 'CpG 2', 'CpG 3']\n",
        "    )\n",
        "    plt.title(f'Coverage Statistics for {tissue}')\n",
        "    plt.ylabel('Coverage')\n",
        "    plt.savefig(f'coverage_{tissue}.png')\n",
        "    plt.show()"
      ],
